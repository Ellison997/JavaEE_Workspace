<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name></display-name>
  <servlet>
    <servlet-name>DoLogin</servlet-name>
    <servlet-class>com.sitcl.mvcoperation.controller.DoLogin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoRegister</servlet-name>
    <servlet-class>com.sitcl.mvcoperation.controller.DoRegister</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoAddition</servlet-name>
    <servlet-class>com.sitcl.mvcoperation.controller.DoAddition</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoUpdate</servlet-name>
    <servlet-class>com.sitcl.mvcoperation.controller.DoUpdate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoDelete</servlet-name>
    <servlet-class>com.sitcl.mvcoperation.controller.DoDelete</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>DoLogin</servlet-name>
    <url-pattern>/DoLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoRegister</servlet-name>
    <url-pattern>/DoRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoAddition</servlet-name>
    <url-pattern>/DoAddition</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoUpdate</servlet-name>
    <url-pattern>/DoUpdate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoDelete</servlet-name>
    <url-pattern>/DoDelete</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>